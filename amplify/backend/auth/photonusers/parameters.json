{
    "identityPoolName": "photon_users",
    "allowUnauthenticatedIdentities": true,
    "lambdaLogPolicy": "photon_0541432b_lambda_log_policy",
    "openIdLambdaRoleName": "photon_0541432b_openid_lambda_role",
    "openIdRolePolicy": "photon_0541432b_openid_pass_role_policy",
