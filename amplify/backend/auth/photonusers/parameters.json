    "mfaPassRolePolicy": "photon0541432b_totp_pass_role_policy",
    "mfaLambdaIAMPolicy": "photon0541432b_totp_lambda_iam_policy",
    "userpoolClientLambdaRole": "photon0541432b_userpoolclient_lambda_role",
